
def add(a, b):
    """
    Returns the sum of a and b.
    """
    return a + b

def subtract(a, b):
    """
    Returns the difference between a and b.
    """
    return a - b

def multiply(a, b):
    """
    Returns the product of a and b.
    """
    return a * b

def divide(a, b):
    """
    Returns the quotient of a divided by b.
    Handles division by zero by printing an error message and returning None.
    """
    if b == 0:
        print("Error: Division by zero")
        return None
    return a / b
